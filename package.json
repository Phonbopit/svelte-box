{
  "name": "sveltekit-box",
  "version": "0.0.1",
  "description": "Learn Svelte",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:ci": "CI=true vitest --coverage",
    "test:coverage": "vitest --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --check . && eslint .",
    "format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --write ."
  },
  "engines": {
    "node": "^18.12 || ^20.9",
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@fontsource/fredoka-one": "5.0.0",
    "@playwright/test": "1.38.0",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/adapter-vercel": "3.1.0",
    "@sveltejs/kit": "1.27.6",
    "@sveltejs/vite-plugin-svelte": "3.0.0",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/svelte": "4.0.3",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitest/coverage-v8": "0.34.4",
    "@vitest/ui": "0.34.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.33.2",
    "jsdom": "22.1.0",
    "postcss": "8.4.24",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vitest": "0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@tanstack/svelte-table": "^8.10.3",
    "motion": "^10.16.4"
  }
}
